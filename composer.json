{
    "name": "anomaly/github_api-extension",
    "type": "streams-addon",
    "description": "Easily integrate GitHub APIs into your Streams Platform projects.",
    "keywords": [
        "streams extension",
        "streams addon",
        "extension"
    ],
    "homepage": "http://anomaly.is/",
    "license": "MIT",
    "authors": [
        {
            "name": "AnomalyLabs, Inc.",
            "email": "hello@anomaly.is",
            "homepage": "http://anomaly.is/",
            "role": "Owner"
        }
    ],
    "support": {
        "email": "support@anomaly.is"
    },
    "require": {
        "knplabs/github-api": "~1.4"
    },
    "autoload": {
        "psr-4": {
            "Anomaly\\GithubApiExtension\\": "src/"
        },
        "psr-0": {
            "Github\\": "vendor/knplabs/github/ap-/lib/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-1.0/master": "1.0.x-dev"
        }
    }
}

